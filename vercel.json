{
    "version": 2,
    "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    }
  ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/route",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTION"]
          },
          {
            "src": "/(?!api)(.*)",
            "dest": "server.js"
          }
    ]
  }